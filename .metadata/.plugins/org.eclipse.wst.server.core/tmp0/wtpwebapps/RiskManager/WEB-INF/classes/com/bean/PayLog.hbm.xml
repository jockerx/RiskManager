<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >  
  
<hibernate-mapping package="com.bean">  
    <class name="PayLog" table="paylog">  
        <id name="id" column="id">  
            <generator class="native"></generator>  
        </id>  
        <property name="member_id" column="member_id" type="java.lang.String" length="45" />
        <property name="pay" column="pay" type="java.lang.Integer" length="12" />
        <property name="date" column="date" type="java.lang.String" length="30" />   
    </class>  
</hibernate-mapping> 