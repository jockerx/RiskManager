<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.bean">
	<class name="Store" table="store">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="name" column="name" type="java.lang.String"
			not-null="true" length="20"></property>
		<property name="address" column="address" type="java.lang.String"
			not-null="true" length="45"></property>
		<property name="telephone" column="telephone" type="java.lang.String"
			not-null="true" length="20"></property>
		<property name="type" column="type" type="java.lang.Integer"
			length="3" />
	</class>
</hibernate-mapping> 