<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.bean">
	<class name="Waiter" table="waiter">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="name" column="name" type="java.lang.String"
			not-null="true" length="45"></property>
		<property name="phone" column="phone" type="java.lang.String"
			not-null="true" length="20"></property>
		<property name="store_id" column="store_id" type="java.lang.Integer"
			length="3" />
		<property name="gender" column="gender" type="java.lang.Integer"
			length="3" />
		<property name="username" column="username" type="java.lang.String"
			not-null="true" length="15"></property>
	</class>
</hibernate-mapping> 